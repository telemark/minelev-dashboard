{
  "name": "minelev-dashboard",
  "description": "Dashboard for MinElev",
  "version": "1.8.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "deploy": "npm test && npm run export && now --static out --name dashboard-minelev && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "cleanup": "rm -rf .next && rm -rf out"
  },
  "keywords": [
    "dashboard",
    "next",
    "status",
    "Telemark fylkeskommune",
    "minElev",
    "Louie"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/minelev-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/minelev-dashboard/issues"
  },
  "homepage": "https://github.com/telemark/minelev-dashboard",
  "now": {
    "name": "dashboard-minelev",
    "alias": [
      "dashboard.minelev.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "1.3.1",
    "coveralls": "3.0.3",
    "nyc": "13.3.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "next": "8.0.3",
    "react-dom": "16.8.3",
    "react": "16.8.3"
  }
}
