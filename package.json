  },
  "homepage": "https://github.com/telemark/minelev-dashboard",
  "now": {
    "alias": [
      "dashboard.minelev.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "coveralls": "2.13.1",
    "nsp": "2.6.3",
    "nyc": "10.3.2",
